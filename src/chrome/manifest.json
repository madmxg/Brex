{
  "manifest_version": 2,
  "name": "replace__APP_NAME__replace",
  "version": "0.0.1",
  "description": "replace__APP_DESCRIPTION__replace",
  "homepage_url": "replace__APP_HOMEPAGE__replace",
  "default_locale": "ru",
  "content_scripts": [{
    "js": ["content.js"],
    "run_at": "document_start",
    "matches": ["<all_urls>"],
    "all_frames": true
  }],
  "web_accessible_resources": ["./*"],
  "background": {
    "js": ["bg.js"]
  },
  "options_page": "options.html",
  "icons": {
    "128": "img/icon.png"
  },
  "permissions": [
    "<all_urls>", 
    "unlimitedStorage",
    "bookmarks",
    "browsingData",
    "contextMenus",
    "downloads",
    "history",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx"
}